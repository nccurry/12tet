  },
  "homepage": "https://github.com/nccurry/musicomatic#readme",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.22.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "ts-results": "^3.3.0"
